{
    "name": "rmn/product-bundle",
    "description": "Product Library",
    "type": "symfony-bundle",
    "version": "0.0.2",
    "require": {
        "php": "^7.1",
        "gedmo/doctrine-extensions": "^2.4",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/http-kernel": "^4.3",
        "symfony/orm-pack": "^1.0",
        "symfony/yaml": "^4.3"
    },
    "require-dev": {
    },
    "config": {
        "bin-dir": "bin",
        "platform": {
            "php": "7.1.3"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "RMN\\ProductBundle\\": "src/"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "RMN\\ProductBundle\\Tests\\": "tests/"
        }
    },
    "conflict": {
        "symfony/symfony": "<4.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
